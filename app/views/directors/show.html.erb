<div class="page-header">
  <h1>Director #<%= @director.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Dob</dt>
  <dd><%= @director.dob %></dd>

  <dt>Name</dt>
  <dd><%= @director.name %></dd>

  <dt>Bio</dt>
  <dd><%= @director.bio %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @director.image_url %>" width="320"></dd>

</dl>

<div><a href="/directors/<%= @director.id %>/edit_form">Edit director</a></div>
<div><a href="/delete_director/<%= @director.id %>">Delete director</a></div>